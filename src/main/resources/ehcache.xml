<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="quiz.domain.User" uses-template="simple"/>

    <cache alias="quiz.domain.Authority" uses-template="simple"/>

    <cache alias="quiz.domain.User.authorities" uses-template="simple"/>

    <cache alias="quiz.domain.PersistentToken" uses-template="simple"/>

    <cache alias="quiz.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="quiz.social.SocialUserConnection" uses-template="simple"/>

    <cache alias="quiz.domain.Avatar" uses-template="simple"/>

    <cache alias="quiz.domain.Player" uses-template="simple"/>

    <cache alias="quiz.domain.Help" uses-template="simple"/>

    <cache alias="quiz.domain.Category" uses-template="simple"/>

    <cache alias="quiz.domain.Question" uses-template="simple"/>


    <cache alias="quiz.domain.Subcategory" uses-template="simple"/>

    <cache alias="quiz.domain.MediaType" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
